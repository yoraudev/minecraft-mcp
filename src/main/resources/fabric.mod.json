{
  "schemaVersion": 1,
  "id": "minecraft-mcp",
  "version": "${version}",
  "name": "minecraft-mcp",
  "description": "A Fabric mod that acts as an MCP server, exposing your Minecraft world to AI assistants through tools and resources.",
  "authors": [
    "Trauma Development"
  ],
  "contact": {},
  "license": "LicenseRef-Trauma-Attribution-No-Rebranding",
  "icon": "assets/minecraft-mcp/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "wtf.yoraudev.mmcp.Core"
    ],
    "main": [
      "wtf.yoraudev.mmcp.Core"
    ]
  },
  "mixins": [
    "mmcp.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-language-kotlin": ">=${kotlin_loader_version}",
    "fabric-api": "*",
    "minecraft": "${minecraft_version}"
  }
}
